<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <Platforms>AnyCPU;x64</Platforms>
    <AssemblyVersion>2.4.1</AssemblyVersion>
    <FileVersion>2.4.1</FileVersion>
    <Version>2.4.1</Version>
    <PackageReleaseNotes>1. Rearrange keyword config;
2. Use "<PackageCopyToOutput>true</PackageCopyToOutput>" to output config files.</PackageReleaseNotes>
    <PackageProjectUrl>https://github.com/victor-wiki/DatabaseManager</PackageProjectUrl>
    <Description>‌Retrieve database objects and generate scripts of them for SqlServer, MySql, Oracle, Postgres and SQLite.</Description>
    <RepositoryUrl></RepositoryUrl>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="BulkCopy\OracleBulkCopy.cs" />
  </ItemGroup>

  <ItemGroup>
    <None Remove="Config\DataTypeSpecification\MySql.xml" />
    <None Remove="Config\DataTypeSpecification\Oracle.xml" />
    <None Remove="Config\DataTypeSpecification\Postgres.xml" />
    <None Remove="Config\DataTypeSpecification\Sqlite.xml" />
    <None Remove="Config\DataTypeSpecification\SqlServer.xml" />
    <None Remove="Config\FunctionSpecification\MySql.xml" />
    <None Remove="Config\FunctionSpecification\Oracle.xml" />
    <None Remove="Config\FunctionSpecification\Postgres.xml" />
    <None Remove="Config\FunctionSpecification\Sqlite.xml" />
    <None Remove="Config\FunctionSpecification\SqlServer.xml" />
    <None Remove="Config\Keyword\MySql.txt" />
    <None Remove="Config\Keyword\Oracle.txt" />
    <None Remove="Config\Keyword\Postgres.txt" />
    <None Remove="Config\Keyword\Sqlite.txt" />
    <None Remove="Config\Keyword\SqlServer.txt" />
    <None Remove="Config\Option\CreateTableOption\MySql.xml" />
    <None Remove="Config\Option\CreateTableOption\Oracle.xml" />
    <None Remove="Config\Option\CreateTableOption\Postgres.xml" />
    <None Remove="Config\Option\CreateTableOption\Sqlite.xml" />
    <None Remove="Config\Option\CreateTableOption\SqlServer.xml" />
    <None Remove="DatabaseInterpreter.Core.csproj.Backup.tmp" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="Config\DataTypeSpecification\MySql.xml">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
	  <PackageCopyToOutput>true</PackageCopyToOutput>
    </Content>
    <Content Include="Config\DataTypeSpecification\Oracle.xml">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
