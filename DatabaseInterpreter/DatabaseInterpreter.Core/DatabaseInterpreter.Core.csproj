	  <PackageCopyToOutput>true</PackageCopyToOutput>
    </Content>
    <Content Include="Config\Option\CreateTableOption\MySql.xml">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
	  <PackageCopyToOutput>true</PackageCopyToOutput>
    </Content>
    <Content Include="Config\Option\CreateTableOption\Oracle.xml">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
	  <PackageCopyToOutput>true</PackageCopyToOutput>
